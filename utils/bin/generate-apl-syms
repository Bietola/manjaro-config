#!/bin/env python3

import fire

def syms():
    res = []
    for i in range(2340, 2377):
        utf_esc_seq = rf'\u{i}'
        utf_sym = bytes(utf_esc_seq, 'utf-8').decode('unicode-escape')

        res.append(utf_sym)

    return res

def dasher_alphabet_group():
    res = []
    for i, apl_sym in enumerate(syms()):
        res.append(f'<s d ="{apl_sym}" t ="{apl_sym}" b="{i}" />')

    return res

def cli(dasher=False):
    if dasher:
        print('\n'.join(dasher_alphabet_group()))
    else:
        print('\n'.join(syms()))

fire.Fire(cli)
